# Server variables for cloud mode
DEPLOY_ENV=cloud
RUN_ON_CLOUD=true
# Preferred on Render: inject from Key Value connection string
REDIS_URL=
# Fallback if not using REDIS_URL
REDIS_HOST=your-render-redis-host
REDIS_PORT=6379
# Either set ORCHESTRATOR_URL or ORCHESTRATOR_HOSTPORT
ORCHESTRATOR_URL=
ORCHESTRATOR_HOSTPORT=your-orchestrator-private-host:4000
CLIENT_URL=https://your-cloudflare-pages-domain.pages.dev
JWT_SECRET=replace-this-secret
